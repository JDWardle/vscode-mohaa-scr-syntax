{
    "name": "vscode-mohaa-scr-syntax",
    "displayName": "MOHAA Morpheus Script (SCR) Syntax Grammar",
    "description": "Syntax highlighting for the Medal of Honor Allied Assault Morpheous Script (scr) files",
    "version": "0.0.1",
    "publisher": "JDWardle",
    "license": "MIT",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/JDWardle/vscode-mohaa-scr-syntax.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "scr",
                "aliases": [
                    "MOHAA",
                    "Morpheus Script",
                    "scr"
                ],
                "extensions": [
                    ".scr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "scr",
                "scopeName": "source.scr",
                "path": "./syntaxes/scr.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.13.1"
    }
}
